{
	"name": "Sketch Palettes",
	"description": "Plugin to load palettes into color picker",
	"author": "Andrew Fiorillo",
	"homepage": "http://github.com/andrewfiorillo/sketch-palettes",
	"version": "1.0",
	"identifier": "com.andrewfiorillo.sketch.sketch-palettes",
	"compatibleVersion": "3",
	"bundleVersion": "1",
	"commands": [
		{
			"name": "Load Palette...",
			"identifier": "loadPalette",
			"shortcut": "",
			"script": "sketchPalettes.js",
			"handler": "loadPalette"
		},
		{
			"name": "Clear Palette",
			"identifier": "clearPalette",
			"shortcut": "",
			"script": "sketchPalettes.js",
			"handler": "clearPalette"
		},
		{
			"name": "Save Palette...",
			"identifier": "savePalette",
			"shortcut": "",
			"script": "sketchPalettes.js",
			"handler": "savePalette"
		},
		{
			"name": "Create Palette...",
			"identifier": "createPalette",
			"shortcut": "",
			"script": "sketchPalettes.js",
			"handler": "createPalette"
		}
	],
	"menu": {
		"items": [
			"loadPalette",
			"clearPalette",
			"savePalette",
			"createPalette"
		]
	}
}